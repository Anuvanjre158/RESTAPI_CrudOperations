#spring.datasource.url=jdbc:mysql://localhost:3306/usersDB?useSSL=false
spring.datasource.driver-class-name=com.mysql.jdbc.Driver
spring.datasource.url=jdbc:mysql://localhost:3306/usersDB
spring.datasourse.username=root
spring.datasourse.password=Root

#Hibernate Properties
# The SQL dialect makes hibernate generate better SQL for the choosenn database
spring.jpa.properties.hibernate.dialect=org.hibernate.dialect.MySQL5InnoDBDialect

#Hibernate ddl auto (create, create-drop, validate, update)
spring.jpa.hibernate.ddl-auto=update
